---
layout: none
---           
{
"specificationVersion": "0.18",
"aid": "{{ site.node }}",
"name": "{{ site.name }}",
"description": "{{ site.description | strip_html }}",
"image": "{{ site.image }}",
"url": "{{ site.url }}/apis.json",
"created": "2024/04/13",
"modified": "2024/04/13",
"tags": {{ site.keywords | jsonify }},
"includes": [{% for api in site.apis %}{% if api.url contains "overlays" %}{% else %}{% if api.url contains "properties" %}{% else %}{% if api.url contains "json" %}{% else %}{% assign pos = api.url | size | minus: 1 %}{% assign len = api.url | size %}{% assign url = api.url | slice: 0, pos %}{% assign url = url | slice: 1, len %} 
    {
    "name": "{{ api.name | strip_newlines }}",
    "url": "https://raw.githubusercontent.com/api-search/{{ site.node }}/main/_{{ url }}.md"
    }{% unless forloop.last %},{% endunless %}{% endif %}{% endif %}{% endif %}{% endfor %}
],
"network": [{% for network in site.data.apis.network %}
    {
        "name": "{{ network.name }}",
        "url": "{{ network.url }}"
    }{% unless forloop.last %},{% endunless %}{% endfor %}
    ],
"maintainers": [{% for maintainer in site.data.apis.maintainers %}
    {
        "FN": "{{ maintainer.FN }}",
        "email": "{{ maintainer.email }}"
    }{% unless forloop.last %},{% endunless %}{% endfor %}
    ]   
}
